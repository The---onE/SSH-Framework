<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-catalog="framework" match-name="admin" />
	<table-filter match-catalog="framework" match-name="log" />
	<table-filter match-catalog="framework" match-name="user" />
	<table catalog="framework" name="admin">
		<column name="id"></column>
		<column name="password"></column>
		<column name="username"></column>
		<column name="create_time"></column>
		<column name="state"></column>
	</table>
	<table catalog="framework" name="log">
		<column name="id"></column>
		<column name="user"></column>
		<column name="type"></column>
		<column name="create_time"></column>
		<column name="method"></column>
		<column name="ip"></column>
		<column name="user_type"></column>
		<column name="target"></column>
		<column name="param"></column>
	</table>
	<table catalog="framework" name="user">
		<column name="id"></column>
		<column name="sex"></column>
		<column name="email"></column>
		<column name="birthday"></column>
		<column name="state"></column>
		<column name="phone"></column>
		<column name="username"></column>
		<column name="password"></column>
		<column name="create_time"></column>
	</table>
</hibernate-reverse-engineering>